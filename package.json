{
  "name": "@karameht/a11y-lens",
  "version": "0.0.1-alpha.2",
  "description": "Real-time accessibility checks for React app based on axe-core",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "sideEffects": false,
  "keywords": [
    "react",
    "accessibility",
    "a11y",
    "axe-core",
    "devtools",
    "panel",
    "overlay",
    "development",
    "vite",
    "nextjs",
    "remix",
    "astro",
    "framework-agnostic"
  ],
  "author": {
    "name": "karameht",
    "url": "https://karameht.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/karameht/a11y-lens.git"
  },
  "bugs": {
    "url": "https://github.com/karameht/a11y-lens/issues"
  },
  "homepage": "https://github.com/karameht/a11y-lens#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build:ts": "tsc",
    "build:css": "postcss src/styles.css -o dist/styles.css",
    "build:js": "find dist -name '*.js' -type f -exec terser {} --compress --mangle --keep-fnames -o {} \\;",
    "build": "npm run build:ts && npm run build:css && npm run build:js",
    "clean": "rm -rf dist",
    "ship": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "react": "^17 || ^18 || ^19",
    "react-dom": "^17 || ^18 || ^19"
  },
  "dependencies": {
    "axe-core": "^4.10.3"
  },
  "devDependencies": {
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "cssnano": "^7.1.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "terser": "^5.43.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
